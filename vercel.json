{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/article/:slug",
            "destination": "/api/article"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}